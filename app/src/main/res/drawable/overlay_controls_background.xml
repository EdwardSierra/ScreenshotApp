<?xml version="1.0" encoding="utf-8"?>
<!--
  Draws a rounded rectangle background for overlay control widgets.
  Inputs: None.
  Outputs: Shape drawable used for overlay control backgrounds.
-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="@color/overlayBackground" />
    <corners android:radius="16dp" />
    <padding
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp" />
</shape>
